{
    "name": "workspace-webui",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "test": "jest --testTimeout 20000",
        "test:ci": "CI=true jest --testTimeout 20000 --env=jsdom",
        "lint": "eslint 'src/**/*.{tsx,ts}'",
        "coverage": "jest --testTimeout 20000 --env=jsdom --coverage",
        "coverage:ci": "CI=true jest --testTimeout 20000 --env=jsdom --coverage && cat coverage/lcov.info | coveralls",
        "debug:chrome": "google-chrome --remote-debugging-port=9222",
        "docs": "typedoc --out docs ./src"
    },
    "dependencies": {
        "ethers": "4.0.47",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-router-dom": "5.1.2"
    },
    "devDependencies": {
        "@types/enzyme": "3.10.5",
        "@types/enzyme-adapter-react-16": "1.0.6",
        "@types/jest": "25.2.1",
        "@types/node": "14.0.1",
        "@types/react": "16.9.34",
        "@types/react-dom": "16.9.8",
        "@types/react-router-dom": "5.1.5",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/eslint-plugin-tslint": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "coveralls": "3.1.0",
        "delay": "4.3.0",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.2",
        "eslint": "^6.1.0",
        "eslint-plugin-jsdoc": "^25.4.2",
        "eslint-plugin-prefer-arrow": "^1.2.1",
        "jest": "25.4.0",
        "react-scripts": "3.4.1",
        "ts-jest": "25.4.0",
        "tslint": "6.1.2",
        "tslint-react": "5.0.0",
        "typedoc": "0.17.6",
        "typescript": "3.9.2",
        "typescript-tslint-plugin": "0.5.5"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
